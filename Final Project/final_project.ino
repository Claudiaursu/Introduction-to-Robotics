  }

  if (dist < 2000 && enaughSpeed) {
    enaughSpeed = false;
    startPlayback(sound2, sizeof(sound2));
    isPlayingSound = true;
    startSound = millis();
    Serial.println("knock");
  }
 
}

void setup(){
 //setup code to start the first MPU accelerometer
  Wire.begin();
  Wire.beginTransmission(MPU1);
  Wire.write(0x6B);  
  Wire.write(0);    
  Wire.endTransmission(true);

 //setup code to start the second MPU accelerometer
  Wire.begin();
  Wire.beginTransmission(MPU2);
  Wire.write(0x6B);  
  Wire.write(0);     
  Wire.endTransmission(true);
  
  Serial.begin(9600);

  getStick1Values(MPU1);
  getStick2Values(MPU2);
}


void loop(){
  checkToSing1(stick1X);
  checkToSing2(stick2X);
}
